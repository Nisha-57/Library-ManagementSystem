	var gr = new GlideRecord('x_574429_library_m_request_table');
	var result={};
	var body=[];
	gr.addQuery('sys_id',sysID);
	//gr.addQuery('gr.u_glide_date_2','<','gr.u_glide_date_1');
	gr.query();
	if(gr.next())
		{
			result.customer_id=gr.customer_id;
			result.customer_name=gr.customer_name;
			result.actual_date=gr.u_glide_date_2;
			result.extending_date=gr.u_glide_date_1;
			if(gr.u_glide_date_2 <= gr.u_glide_date_1)
				{
					result.u_string_3="Accepted";
				}
			else
				{
					result.u_string_3="Rejected";
				}
		body.push(result);
		}
	response.setBody(body);

})(request, response);]]></operation_script>
